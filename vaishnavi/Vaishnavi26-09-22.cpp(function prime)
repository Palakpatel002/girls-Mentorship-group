//prime number by function call
#include<iostream>
using namespace std;

int primenumber(int n){
    for(int i=2;i<n;i++){
        if(n==2||n%i!=0){
            cout<<" number is a prime number";
            break;
        }
        else{
             cout<<"number is not a prime number";
             break;
        }
    }
}
int main(){
    int n;
    cin>>n;
    primenumber(n);
}
