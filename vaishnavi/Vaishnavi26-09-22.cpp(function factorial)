#include<iostream>
using namespace std;

int factorial(int n){
    
    int a=1;
    for(int i=1;i<=n;i++)
    {
     a=a*i;
    }
    return a;
}

int main(){
    int n;
    cin>>n;
    factorial(n);
    cout<<factorial(n);
}                                                                                                   
