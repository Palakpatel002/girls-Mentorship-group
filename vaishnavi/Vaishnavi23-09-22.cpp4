// Print Pattern for numbers of rows and column
// * * * * 
// * # * #
// * * * *
// * # * #
#include<iostream>
using namespace std;
int main()
{
    int i,j,n;
    cin>>n;

    for(i=1;i<n;i++){
        for(j=1;j<n;j++){
            if(i%2==0&&j%2==0){
                cout<<"# ";
            }
            else{
                cout<<"* ";
            }
        }
        cout<<endl;
    }
}
